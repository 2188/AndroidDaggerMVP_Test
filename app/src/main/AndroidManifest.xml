<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.demo_dagger_rxandroid_retrofit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MasterDetailListActivity"
            android:label="@string/title_masterdetail_list"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".MasterDetailDetailActivity"
            android:label="@string/title_masterdetail_detail"
            android:parentActivityName=".MasterDetailListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.demo_dagger_rxandroid_retrofit.MasterDetailListActivity" />
        </activity>
    </application>

</manifest>